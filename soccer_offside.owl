                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&Soccer_Offside;hasTeammate"/>
                        <owl:someValuesFrom rdf:resource="&Soccer_Offside;BallLastToucher"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="&Soccer_Offside;Player"/>
    </owl:Class>
    


