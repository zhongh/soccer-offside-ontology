    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY Soccer_Offside "http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/" >
]>


<rdf:RDF xmlns="http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/"
     xml:base="http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:Soccer_Offside="http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/">
        <owl:versionIRI rdf:resource="&Soccer_Offside;1.0.0"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/hasPosition -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;hasPosition">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="&Soccer_Offside;Position"/>
        <owl:inverseOf rdf:resource="&Soccer_Offside;isPositionOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Soccer_Offside;Ball"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Glove"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Player"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Referee"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/hasTeam -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;hasTeam">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&Soccer_Offside;Player"/>
        <rdfs:range rdf:resource="&Soccer_Offside;Team"/>
        <owl:inverseOf rdf:resource="&Soccer_Offside;isTeamOf"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/hasTeammate -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;hasTeammate">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:domain rdf:resource="&Soccer_Offside;Player"/>
        <rdfs:range rdf:resource="&Soccer_Offside;Player"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/isInvolvedIn -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;isInvolvedIn">
        <rdfs:range rdf:resource="&Soccer_Offside;ActivePlay"/>
        <rdfs:domain rdf:resource="&Soccer_Offside;Player"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/isNearerToDefenderGoalLineThan -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;isNearerToDefenderGoalLineThan">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Soccer_Offside;Ball"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Player"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Position"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Soccer_Offside;Ball"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Player"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Position"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/isOpponentOf -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;isOpponentOf">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:range rdf:resource="&Soccer_Offside;Player"/>
        <rdfs:domain rdf:resource="&Soccer_Offside;Player"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/isPositionOf -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;isPositionOf">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&Soccer_Offside;Position"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Soccer_Offside;Ball"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Glove"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Player"/>
                    <rdf:Description rdf:about="&Soccer_Offside;Referee"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/isTeamOf -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;isTeamOf">
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="&Soccer_Offside;Player"/>
        <rdfs:domain rdf:resource="&Soccer_Offside;Team"/>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/playsAgainst -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;playsAgainst">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:domain rdf:resource="&Soccer_Offside;Player"/>
        <rdfs:range rdf:resource="&Soccer_Offside;Player"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&Soccer_Offside;hasTeammate"/>
            <rdf:Description rdf:about="&Soccer_Offside;isOpponentOf"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/touches -->

    <owl:ObjectProperty rdf:about="&Soccer_Offside;touches">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&Soccer_Offside;Ball"/>
        <rdfs:domain rdf:resource="&Soccer_Offside;Player"/>
    </owl:ObjectProperty>
    


