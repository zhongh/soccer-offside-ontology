    <!-- http://tw.rpi.edu/web/Ontologies/2016/Soccer_Offside/ActivePlay -->

    <owl:Class rdf:about="&Soccer_Offside;ActivePlay">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&Soccer_Offside;ball_touch"/>
